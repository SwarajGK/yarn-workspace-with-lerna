  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
